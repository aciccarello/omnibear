{
  "manifest_version": 2,
  "name": "Omnibear",
  "version": "0.2.0",
  "permissions": [
     "contextMenus",
     "http://omnibear.com/",
     "http://*/",
     "https://*/",
     "tabs"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "logo-70.png",
    "48": "logo-70.png",
    "128": "logo-190.png"
  }
}
